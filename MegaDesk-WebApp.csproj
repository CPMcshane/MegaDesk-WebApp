    <ImplicitUsings>enable</ImplicitUsings>
    <RootNamespace>MegaDesk_WebApp</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="6.0.10" />
